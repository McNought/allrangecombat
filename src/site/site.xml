<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="OBSIDIAN WEAPON">

    <!-- See: http://maven.apache.org/skins/ -->
  <skin>
    <groupId>lt.velykis.maven.skins</groupId>
    <artifactId>reflow-maven-skin</artifactId>
    <version>1.1.1</version>
  </skin>

  <custom>
    <reflowSkin>
      <theme>bootswatch-flatly</theme>
      <highlightJs>true</highlightJs>
      <brand>
        <name>A.R.C.S.</name>
        <href>index.html</href>
      </brand>
      <slogan>A Downloadable Development Environment for IPL</slogan>
      <titleTemplate>%2$s | %1$s</titleTemplate>
      <toc>top</toc>
      <topNav>Download|reports</topNav>
      <bottomNav>
        <column>Main|Download</column>
        <column>Documentation</column>
        <column>reports|modules</column>
      </bottomNav>
      <bottomDescription>
        An easy way to set up developement environments for projects.
        Easy and modern (sets up tools for a VM).
      </bottomDescription>
      <pages>
        <index project="project-id">
          <shortTitle>Welcome</shortTitle>
          <breadcrumbs>false</breadcrumbs>
          <toc>false</toc>
          <sections>
            <carousel />
            <body />
            <sidebar />
            <thumbs>2</thumbs>
            <columns>3</columns>
          </sections>
        </index>
        <developer-info>
          <toc>sidebar</toc>
        </developer-info>
      </pages>
    </reflowSkin>
  </custom>


